{
  "name": "gatsby-starter-pikku-i18n",
  "description": "I18n boilerplate project with pikku-i18n for GatsbyJS",
  "version": "1.0.1",
  "author": "Mikko Vänskä <mikkovanska.com@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "gatsby": "^2.19.49",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-mdx": "^1.1.1",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-plugin-sitemap": "^2.2.30",
    "gatsby-source-filesystem": "^2.1.57",
    "gatsby-transformer-sharp": "^2.3.19",
    "i18next": "^17.3.1",
    "i18next-xhr-backend": "^3.2.2",
    "pikku-i18n": "^v2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.13.2"
  },
  "devDependencies": {
    "gatsby-plugin-create-i18n-mdx-pages": "github:vanska/gatsby-plugin-create-i18n-mdx-pages#v1.0.0",
    "gatsby-plugin-generate-i18n-navigation": "github:vanska/gatsby-plugin-generate-i18n-navigation#v1.0.1",
    "gatsby-plugin-i18n-pages-from-json": "github:vanska/gatsby-plugin-i18n-pages-from-json#v1.0.2",
    "gatsby-plugin-transform-i18n-locales": "github:vanska/gatsby-plugin-transform-i18n-locales#v1.1.3",
    "gatsby-plugin-transform-site-config-json": "github:vanska/gatsby-plugin-transform-site-config-json#v1.0.2",
    "glob": "^7.1.6",
    "log-beautify": "^1.2.0",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean-build": "gatsby clean && npm run build",
    "develop": "gatsby develop",
    "develop-stage": "GATSBY_ACTIVE_ENV=staging gatsby develop",
    "develop-https": "gatsby develop -p 8000 --https",
    "develop-nat": "gatsby develop -H 0.0.0.0 -p 8000",
    "develop-stage-nat": "GATSBY_ACTIVE_ENV=staging npm run develop-nat",
    "format": "prettier --write src/**/*.{js,jsx}",
    "clear": "gatsby clean",
    "clean-develop": "gatsby clean && npm run develop",
    "clean-develop-nat": "gatsby clean && npm run develop-nat",
    "clean-develop-https": "gatsby clean && npm run develop-https",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-firebase": "firebase serve --only hosting",
    "serve-firebase-nat": "firebase serve --only hosting --host 0.0.0.0 --port 8000",
    "clean-build-serve": "npm run clean-build && npm run serve-firebase",
    "clean-build-serve-nat": "npm run clean-build && npm run serve-firebase-nat",
    "build-serve": "npm run build && npm run serve-firebase",
    "clean-deploy": "npm run clean-build && npm run deploy",
    "serve-nat": "gatsby serve -H 0.0.0.0 -p 8000",
    "deploy": "firebase deploy",
    "stage-build": "GATSBY_ACTIVE_ENV=staging npm run build",
    "stage-build-serve-nat": "npm run clear && npm run stage-build && npm run serve-firebase-nat",
    "stage-build-deploy": "npm run clear && npm run stage-build && npm run deploy",
    "production-build": "GATSBY_ACTIVE_ENV=production npm run build",
    "production-build-serve-nat": "npm run clear && npm run production-build && npm run serve-firebase-nat",
    "production-build-deploy": "npm run clear && npm run production-build && npm run deploy",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanska/gatsby-starter-pikku-i18n.git"
  }
}
